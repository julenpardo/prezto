# Docker

function docker_bash() {
    docker exec -it $1 /bin/bash
}

function docker_rmi_none() {
    docker rmi -f $(docker images -q -f "dangling=true")
}

function docker_rm_all() {
    docker rm -f $(docker ps -aq)
}
